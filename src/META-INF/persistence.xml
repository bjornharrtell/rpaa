<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="rpaa">
		<jta-data-source>jdbc/rpaa</jta-data-source>
		<class>entities.grid.Category</class>
		<class>entities.grid.Subject</class>
		<class>entities.grid.Vote</class>
		<!-- 
		<class>entities.tree.Edge</class>
		<class>entities.tree.SubjectVertex</class>
		<class>entities.tree.Vertex</class>
		 -->
		<properties>
			<property name="eclipselink.jdbc.native-sql" value="true"/>
		</properties>
	</persistence-unit>
</persistence>
